local queueteleport = queue_on_teleport or (syn and syn.queue_on_teleport) or (fluxus and fluxus.queue_on_teleport)

-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤ Auto-Reload
if queueteleport then
	queueteleport([[
        loadstring(game:HttpGet("https://raw.githubusercontent.com/tawlavegas-jpg/wdwdwdwd/refs/heads/main/tththth"))()
	]])
	print("‚úÖ Auto-reload enabled")
else
	warn("‚ö†Ô∏è queueteleport not supported - script will not auto-reload after rejoin")
end

local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local PlaceId = game.PlaceId
local JobId = game.JobId

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô Rejoin
local function rejoin()
    print("üîÑ ‡∏Å‡∏≥‡∏•‡∏±‡∏á Rejoin...")
    task.wait(1)
    
    if #Players:GetPlayers() <= 1 then
        -- ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô <= 1 ‡∏Ñ‡∏ô (‡πÅ‡∏Ñ‡πà‡∏ï‡∏±‡∏ß‡πÄ‡∏£‡∏≤‡πÄ‡∏≠‡∏á)
        Players.LocalPlayer:Kick("\nRejoining...")
        task.wait()
        TeleportService:Teleport(PlaceId, Players.LocalPlayer)
    else
        -- ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏°‡∏≤‡∏Å‡∏Å‡∏ß‡πà‡∏≤ 1 ‡∏Ñ‡∏ô ‡∏Å‡∏•‡∏±‡∏ö‡πÄ‡∏Ç‡πâ‡∏≤‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡πÄ‡∏ß‡∏≠‡∏£‡πå‡πÄ‡∏î‡∏¥‡∏°
        TeleportService:TeleportToPlaceInstance(PlaceId, JobId, Players.LocalPlayer)
    end
end

-- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏´‡∏•‡∏±‡∏Å
local Role = "Kamado"
local player = Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

-- ‡∏£‡∏≠‡πÉ‡∏´‡πâ LoadingScreen ‡∏´‡∏≤‡∏¢
print("‚è≥ ‡∏£‡∏≠ LoadingScreen ‡∏´‡∏≤‡∏¢...")
repeat task.wait(0.5) until not PlayerGui:FindFirstChild("LoadingScreen")
print("‚úÖ LoadingScreen ‡∏´‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß!")

local Interface = PlayerGui:WaitForChild("Interface")
local Customization = Interface:WaitForChild("Customization")
local GuiFamily = Customization:WaitForChild("Family")
local SpinsText = GuiFamily:WaitForChild("Spins")

local asy = {"Character", "Customization", "Eyes", 5}
local bug = {"Character", "Customization", "Eyes", workspace}

_G.autoRole = true

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏î‡∏∂‡∏á‡∏à‡∏≥‡∏ô‡∏ß‡∏ô Chances
local function getChances()
    local text = SpinsText.Text
    local number = tonumber(string.match(text, "%d+"))
    return number or 0
end



-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏´‡∏•‡∏±‡∏Å
autoRole = function()
    local round = 0
    local foundRole = false
    
    while _G.autoRole do
        local currentChances = getChances()
        
        if currentChances <= 0 then
            print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
            print("‚ö†Ô∏è ‡∏´‡∏°‡∏î Chances ‡πÅ‡∏•‡πâ‡∏ß!")
            print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
            break
        end
        
        round = round + 1
        print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
        print("üîÑ ‡∏£‡∏≠‡∏ö‡∏ó‡∏µ‡πà " .. round .. " (‡πÄ‡∏´‡∏•‡∏∑‡∏≠ " .. currentChances .. " Chances)")
        print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
        
        task.wait(0.1)
        
        -- ‡πÄ‡∏ä‡πá‡∏Ñ‡∏ß‡πà‡∏≤‡πÑ‡∏î‡πâ Role ‡∏´‡∏£‡∏∑‡∏≠‡∏¢‡∏±‡∏á
        if string.find(GuiFamily.Text, Role) then
            print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
            print("‚úÖ ‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à! ‡πÑ‡∏î‡πâ " .. Role .. " ‡πÅ‡∏•‡πâ‡∏ß!")
            print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Sync"):InvokeServer(unpack(asy))
            foundRole = true
            break
        else
            print("  ‚öôÔ∏è ‡∏Å‡∏≥‡∏•‡∏±‡∏á Reroll 50 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á...")
            
            -- Reroll 50 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á
            for i = 1, 50 do
                if not _G.autoRole then break end
                
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Sync"):InvokeServer(unpack(bug))
                
                -- ‡πÅ‡∏™‡∏î‡∏á‡∏Ñ‡∏ß‡∏≤‡∏°‡∏Ñ‡∏∑‡∏ö‡∏´‡∏ô‡πâ‡∏≤‡∏ó‡∏∏‡∏Å 10 ‡∏Ñ‡∏£‡∏±‡πâ‡∏á
                if i % 10 == 0 then
                    print("  ‚Ü≥ " .. i .. "/50")
                end
                
                task.wait(0.05)  -- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏à‡∏≤‡∏Å 0.001 ‡πÄ‡∏õ‡πá‡∏ô 0.05 ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏Ñ‡∏ß‡∏≤‡∏°‡∏õ‡∏•‡∏≠‡∏î‡∏†‡∏±‡∏¢
            end
            
            task.wait(1)
            print("  üìã ‡∏™‡πà‡∏á LastName request")
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Sync"):InvokeServer("Character","Customization","LastName")
            
            task.wait(0.5)  -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ UI ‡∏≠‡∏±‡∏û‡πÄ‡∏î‡∏ó
        end
    end
    
    print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
    
    -- Rejoin ‡πÄ‡∏â‡∏û‡∏≤‡∏∞‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏´‡∏°‡∏î Chances ‡πÅ‡∏•‡∏∞‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ Role
    if not foundRole then
        print("üò¢ ‡πÑ‡∏°‡πà‡πÑ‡∏î‡πâ " .. Role .. " - ‡∏Å‡∏≥‡∏•‡∏±‡∏á Rejoin...")
        rejoin()
    else
        print("üéâ ‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏°‡∏ö‡∏π‡∏£‡∏ì‡πå! ‡πÑ‡∏î‡πâ " .. Role .. " ‡πÅ‡∏•‡πâ‡∏ß")
        print("üõë ‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô")
    end
    
    print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
end

-- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô
autoRole()
