    task.wait(0.001)
until gui:FindFirstChild("LoadingScreen") == nil

print("LoadingScreen ‡∏´‡∏≤‡∏¢‡πÅ‡∏•‡πâ‡∏ß ‚Äî ‡πÑ‡∏õ‡∏ï‡πà‡∏≠‡πÑ‡∏î‡πâ!")


local Roll = "Kamado"
local clanme = nil
local Randomchance = nil
local rmRB = game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Sync")

local Players = game:GetService("Players")
local player = Players.LocalPlayer 
local TeleportService = game:GetService("TeleportService")
local PlaceId = game.PlaceId
local JobId = game.JobId






local normalvalue = {"Character","Customization","Eyes",9}
local errorvalue = { "Character","Customization","Eyes",workspace}


CheckRoll = function()
    for _,v in next, getgc(true) do
        if type(v) == "table" and rawget(v, "LastName") then
            clanme = v.LastName -- string
            Randomchance = v.FamilyRolls
            break
        end
    end
end

-- print(type(Randomchance))
-- setclipboard(tostring(clanme))

-----------------------------------------------------
-- REJOIN FUNCTION
-----------------------------------------------------

rejoin = function()
    print("üîÑ Rejoining...")
    task.wait(1)

    if #Players:GetPlayers() <= 1 then
        player:Kick("\nRejoining...")
        task.wait()
        TeleportService:Teleport(PlaceId, player)
    else
        TeleportService:TeleportToPlaceInstance(PlaceId, JobId, player)
    end
end



SpinRoll = function()
    if clanme == Roll then
        print("Name:" .. clanme)
    elseif clanme ~= Roll and Randomchance > 0 then
        print("‡∏™‡∏∏‡πà‡∏°‡∏≠‡∏µ‡∏Å")
    end
end

_G.SpinRoll = true
while _G.SpinRoll do
    -- SpinRoll
    CheckRoll()
    if clanme == Roll then
        local f = 0
        repeat
            f = f + 1
            rmRB:InvokeServer(unpack(normalvalue))
            print("Normalizing... " .. f .. "/10")
            task.wait(0.01)  -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô lag
        until f >= 10
        break
    elseif clanme ~= Roll and Randomchance > 0 then
        local i = 0
        repeat
            i = i + 1 
            rmRB:InvokeServer(unpack(errorvalue))
            task.wait(0.001)
        until i >= 60
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Sync"):InvokeServer("Character","Customization","LastName")
    elseif Randomchance <= 0  and clanme ~= Roll then
            print("‡∏≠‡∏≠‡∏Å")
            rejoin()
    elseif Randomchance <= 0  and clanme == Roll then
         repeat
            a = a + 1
            rmRB:InvokeServer(unpack(normalvalue))
            print("Normalizing... " .. a .. "/10")
            task.wait(0.01)  -- ‡∏õ‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ô lag
        until a >= 10
        break
    end
    task.wait(0.001)
end




